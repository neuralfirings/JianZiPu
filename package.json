{
  "name": "jianzipu",
  "version": "2.0.0",
  "source": "src/jianzipu.js",
  "main": "dist/jianzipu.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "targets": {
    "frontend": {
      "source": "src/index.html",
      "distDir": "web"
    },
    "backend": {
      "source": "src/jianzipu.js",
      "outputFormat": "esmodule",
      "isLibrary": true,
      "distDir": "dist"
    }
  },
  "scripts": {
    "dev": "parcel src/index.html --dist-dir dev -p 1664",
    "build": "parcel build --public-url ./",
    "watch": "parcel watch"
  },
  "author": "Nancy Yi Liang",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  }
}
